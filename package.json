{
  "name": "videojs-playbackrate-adjuster",
  "version": "1.0.0",
  "description": "A Video.js 6 middleware that adjusts controls based on playback rate",
  "main": "dist/es5/index.js",
  "scripts": {
    "build":"npm-run-all build:first ngcopy",
    "build:first": "sb-build",
    "clean": "sb-clean",
    "lint": "sb-lint",
    "start": "sb-start",
    "test": "sb-test",
    "watch": "sb-watch",
    "prepublish": "npm run build",
    "postversion": "sb-release",
    "ngcopy": "node -e \"var s=require('shelljs');d=['dist/videojs-playbackrate-adjuster.js','dist/videojs-playbackrate-adjuster.min.js'];s.cp('-r',d,'../js/');\""
  },
  "keywords": [
    "videojs",
    "videojs-middleware",
    "videojs-plugin"
  ],
  "author": "Gary Katsevman <me@gkatsev.com>",
  "license": "MIT",
  "jsnext:main": "src/js/index.js",
  "engines": {
    "node": ">=4.4.0"
  },
  "generator-videojs-plugin": {
    "version": "3.1.1"
  },
  "browserify-shim": "node_modules/videojs-spellbook/config/shim.config.js",
  "spellbook": {
    "css": false,
    "lang": false,
    "docs": false
  },
  "files": [
    "CHANGELOG.md",
    "CONTRIBUTING.md",
    "README.md",
    "dist/browser",
    "dist/docs",
    "dist/es5",
    "dist/lang",
    "docs/",
    "index.html",
    "src/"
  ],
  "dependencies": {
    "video.js": "^6.0.0-RC.5"
  },
  "devDependencies": {
    "videojs-spellbook": "^4.0.1"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videojs/videojs-playbackrate-adjuster.git"
  },
  "bugs": {
    "url": "https://github.com/videojs/videojs-playbackrate-adjuster/issues"
  },
  "homepage": "https://github.com/videojs/videojs-playbackrate-adjuster#readme"
}
